extends(src='layout.sgr')
  block(name='content')

    .panel
      .panel-heading
        .panel-heading-left
          h2 Dayparting
          .panel-description Descriptive text. Note about time zones following DV formatting standards for internationalization.
      .panel-body
        select(ng-model='selectedItem' ng-options='item.value as item.label disable when item.disabled for item in items' ng-change='selectedItemChanged()')
        angular-dayparts(options='options')

